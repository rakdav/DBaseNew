<Window x:Class="DBase.AddPay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBase"
        mc:Ignorable="d"
        Title="Начисление зарплаты" Height="450" Width="400" Background="HotPink">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <DatePicker x:Name="Birthday" Margin="10 10 10 10" Grid.Row="2"/>
        <TextBox x:Name="Sum" Margin="10 10 10 10" Grid.Row="3"/>
        <ComboBox x:Name="Staffs" Margin="10 10 10 10" Grid.Row="0">
          
        </ComboBox>
        <ComboBox x:Name="Item_Pays" Margin="10 10 10 10" Grid.Row="1">

        </ComboBox>
        <StackPanel Grid.Row="4" Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="OK" Content="OK" Width="100" Margin="10 5 10 5" Click="OK_Click"/>
            <Button x:Name="Cancel" Content="Cancel" Width="100" Margin="10 5 10 5" Click="Cancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
